{
  "scripts": {
    "asbuild:untouched": "asc assembly/index.ts -b build/untouched.wasm -t build/untouched.wat --validate --sourceMap --debug",
    "asbuild:optimized": "asc assembly/index.ts -b build/optimized.wasm -t build/optimized.wat --validate --sourceMap --optimize",
    "asbuild": "npm run asbuild:untouched && npm run asbuild:optimized",
    "test": "node tests",
    "basic-example": "npx asc ./assembly/index.ts --binaryFile ./build/optimized.wasm",
    "oroneg": "tsc Oron.ts && node Oron assembly/index.ts assembly/oronAdvice.ts assembly/output.ts",
    "fapp": "npx asc assembly/functionApply.ts -o functionApply.wasm --enable reference-types"
  },
  "dependencies": {
    "@assemblyscript/loader": "^0.9.4",
    "@types/node": "^13.9.5",
    "ts-node": "^8.8.1",
    "typescript": "^3.8.3"
  },
  "devDependencies": {
    "assemblyscript": "^0.9.4"
  },
  "repository": {
    "url": "https://github.com/aaronmunsters/Oron"
  },
  "license": "MIT"
}
